      "start_token": 363381,
      "end_token": 363413,
      "sentence_id": 23549,
      "paragraph_id": 0,
      "chapter": 158
    },
    {
      "line_id": "line_000012",
      "character_id": "char_narrator",
      "character_name": "Narrator",
