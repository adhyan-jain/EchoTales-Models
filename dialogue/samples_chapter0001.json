      "start_token": 780,
      "end_token": 797,
      "sentence_id": 57,
      "paragraph_id": 0,
      "chapter": 1
    },
    {
      "line_id": "line_000059",
      "character_id": "char_narrator",
      "character_name": "Narrator",
