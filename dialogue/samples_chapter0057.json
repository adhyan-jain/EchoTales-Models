      "sentence_id": 9107,
      "paragraph_id": 0,
      "chapter": 57
    },
    {
      "line_id": "line_000098",
      "character_id": "char_narrator",
      "character_name": "Narrator",
      "text": "“ An interesting question .",
      "emotion": "neutral",
      "start_token": 136281,
