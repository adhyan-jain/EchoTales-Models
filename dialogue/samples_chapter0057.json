      "sentence_id": 9103,
      "paragraph_id": 0,
      "chapter": 57
    },
    {
      "line_id": "line_000094",
      "character_id": "char_narrator",
      "character_name": "Narrator",
      "text": "Is the Nation of the Evernight related to the Evernight Goddess — is there any connection between the two ?",
      "emotion": "joy",
      "start_token": 136201,
      "end_token": 136220,
      "sentence_id": 9104,
      "paragraph_id": 0,
      "chapter": 57
    },
    {
