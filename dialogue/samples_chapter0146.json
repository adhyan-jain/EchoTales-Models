      "emotion": "neutral",
      "start_token": 337613,
      "end_token": 337629,
      "sentence_id": 21937,
      "paragraph_id": 0,
      "chapter": 146
    },
    {
      "line_id": "line_000070",
      "character_id": "char_narrator",
      "character_name": "Narrator",
