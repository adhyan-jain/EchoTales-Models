      "emotion": "neutral",
      "start_token": 251488,
      "end_token": 251500,
      "sentence_id": 16634,
      "paragraph_id": 0,
      "chapter": 109
    },
    {
      "line_id": "line_000033",
      "character_id": "char_narrator",
      "character_name": "Narrator",
      "text": "We do n’t know the true cause .",
      "emotion": "neutral",
      "start_token": 251501,
      "end_token": 251508,
      "sentence_id": 16635,
      "paragraph_id": 0,
      "chapter": 109
    },
    {
      "line_id": "line_000034",
      "character_id": "char_narrator",
      "character_name": "Narrator",
