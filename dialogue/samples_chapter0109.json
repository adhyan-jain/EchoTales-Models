      "emotion": "neutral",
      "start_token": 251532,
      "end_token": 251542,
      "sentence_id": 16637,
      "paragraph_id": 0,
      "chapter": 109
    },
    {
      "line_id": "line_000036",
      "character_id": "char_574",
      "character_name": "Royale",
