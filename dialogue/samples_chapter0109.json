      "emotion": "neutral",
      "start_token": 251509,
      "end_token": 251531,
      "sentence_id": 16636,
      "paragraph_id": 0,
      "chapter": 109
    },
    {
      "line_id": "line_000035",
      "character_id": "char_narrator",
      "character_name": "Narrator",
