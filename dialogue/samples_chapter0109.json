      "emotion": "neutral",
      "start_token": 251473,
      "end_token": 251487,
      "sentence_id": 16633,
      "paragraph_id": 0,
      "chapter": 109
    },
    {
      "line_id": "line_000032",
      "character_id": "char_narrator",
      "character_name": "Narrator",
