      "emotion": "neutral",
      "start_token": 326001,
      "end_token": 326015,
      "sentence_id": 21247,
      "paragraph_id": 0,
      "chapter": 141
    },
    {
      "line_id": "line_000071",
      "character_id": "char_471",
      "character_name": "Dunn",
